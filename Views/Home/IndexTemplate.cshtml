
@{
    ViewData["Title"] = "Home Page";
}
@section styles {
    <% for (var css in htmlWebpackPlugin.files.css) { %>
    <link href="~<%= htmlWebpackPlugin.files.css[css] %>" rel="stylesheet" asp-append-version="true">
    <% } %>
}

<div id="react-app" asp-prerender-module="ClientApp/dist/serverBundle">Loading...</div>

@section scripts {
    <% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
    <script src="~<%= htmlWebpackPlugin.files.chunks[chunk].entry %>" asp-append-version="true"></script>
    <% } %>
}